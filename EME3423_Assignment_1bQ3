import cv2
import numpy as np
capture = cv2.VideoCapture(0, cv2.CAP_DSHOW)
while  True:
    _ , img = capture.read()

    img2 = img
    img1 = cv2.cvtColor(img, cv2.COLOR_BGR2HSV)
    img = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)              #COLOR cord
    imgCanny = cv2.Canny(img,100,100)                         #Size

    kernel = np.ones((5,5), np.uint8)
    imgDilation = cv2.dilate(imgCanny, kernel, iterations=1)  #
    cv2.imshow("img",img2)
    cv2.imshow("webcam", img)                        #img
    cv2.imshow("img2", img1)
    cv2.imshow("imgCanny", imgCanny)                 #imgCanny
    if cv2.waitKey(20) & 0xff == ord('q'):                    #butten
        break

capture.release()
cv2.destroyAllWindows()

#Today learn how to relly using Python
